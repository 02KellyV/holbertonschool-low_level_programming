#include <stdio.h>

int main (void)
{

int lim  = 10;
printf("%d", lim));
}


int sums (int lim)
{
int m3 = 3;
int m5 = 5;
int i = 0;
int R = 0;

while (i != 0)
{
	if (m3 < lim)
	{
		R += m3;
		m3 += 3;
	}
	if (m5 < lim)
	{
		R += m5;
		m5 += 5;
	}
	if (lim <= m3 && lim <= m5)
	{
		i = 1;
	}
}
return(R);
}
